<table>
  <tr>
    <th>
      Long Name
    </th>
    <th>
      Distance
    </th>
    <th>
      Phone Number
    </th>
    <th>
      Store Type
    </th>
  </tr>

  <%= @stores.each do |store| %>
  <tr>
    <td>
      <%= store[:longName] %>
    </td>
    <td>
      <%= store[:distance] %>
    </td>
    <td>
      <%= store[:phone] %>
    </td>
    <td>
      <%= store[:storeType] %>
    </td>
  </tr>
  <% end %>
</table>
